{
  "name": "assume-aws-role-from-gcp-action",
  "version": "0.0.0",
  "private": true,
  "description": "Assume an AWS role from a GCP self hosted github runner",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build src/main.ts --source-map --license licenses.txt",
    "test": "jest",
    "start": "ts-node src/main.ts",
    "all": "yarn format && yarn lint --fix && yarn build && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/descope/assume-aws-role-from-gcp-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "gcp",
    "aws",
    "self-hosted"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.10.1",
    "@aws-sdk/client-sts": "3.540.0",
    "@aws-sdk/credential-provider-node": "^3.540.0",
    "googleapis": "133.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "8.37.0",
    "@typescript-eslint/parser": "8.37.0",
    "@vercel/ncc": "0.38.3",
    "dotenv": "^17.0.0",
    "eslint": "9.31.0",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "30.0.4",
    "js-yaml": "4.1.0",
    "prettier": "3.6.2",
    "ts-jest": "29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "<5.8.4"
  },
  "packageManager": "yarn@4.1.1",
  "engines": {
    "node": "^20"
  }
}
